<div class="clearfix top-space small"></div>
<div class="row">

  <%= render :partial => 'shared/left_nav_user' %>

  <div class="panel panel-default col-md-8 text-left">
    <div class="pull-left">
      <h4>Twets</h4>
    </div>
    <div class="clearfix"></div>
    <hr />
    <ol class="list-unstyled">
    <% @user.twets.order(created_at: :desc).each do |twet| -%>
      <%= render partial: "shared/twet", locals: {twet: twet} %>
    <% end -%>
    </ol>
  </div>
</div>
